<div class="maincontainer">
  <div class="cards-container">
    <%= render 'shared/search-form' %>
    <div class="row">
      <% @boats.each do |b| %>
          <div class="col-xs-12 col-sm-12 col-md-6">
            <%= render "shared/boatcard", b: b  %>
          </div>
      <% end %>
    </div>
  </div>
  <div id="map" data-markers="<%= @markers.to_json %>"></div>
</div>

